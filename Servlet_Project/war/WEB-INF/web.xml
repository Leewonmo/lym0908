<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>Registmember</servlet-name>
		<servlet-class>kr.ac.shinhancsp.RegistMemberServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Registmember</servlet-name>
		<url-pattern>/registmember</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>/RetrieveMemberServlet</servlet-name>
		<servlet-class>kr.ac.shinhancsp./RetrieveMemberServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>/RetrieveMemberServlet</servlet-name>
		<url-pattern>/retrievememberservlet</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
